<h1>Account Setting</h1>
<%= form_for current_user, url: account_setting_path do |f| %>
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :email %>
      <%= current_user.email %>
    </div>
    <div class="field">
      <%= f.submit %>
    </div>
<% end %>
<p><%= link_to "Cancel", account_path %></p>